<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>mars.libs.jME3utilities</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>mars.libs.jMonkeyEngineCore</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.2</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>jme3utilities</package>
                <package>jme3utilities.controls</package>
                <package>jme3utilities.debug</package>
                <package>jme3utilities.debug.test</package>
                <package>jme3utilities.math</package>
                <package>jme3utilities.navigation</package>
                <package>jme3utilities.nifty</package>
                <package>jme3utilities.nifty.test</package>
                <package>jme3utilities.physics</package>
                <package>jme3utilities.sky</package>
                <package>jme3utilities.sky.test</package>
                <package>jme3utilities.sky.textures</package>
                <package>jme3utilities.ui</package>
                <package>jme3utilities.xml</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/jME3-utilities.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/jME3-utilities.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
